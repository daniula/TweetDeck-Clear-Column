{
  "name": "TweetDeck - Clear Column Shortcut",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension adds keyboard shortcut for clearing tweets in selected column.",
  "homepage_url": "http://twitter.com/daniula",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://tweetdeck.twitter.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://tweetdeck.twitter.com/"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}